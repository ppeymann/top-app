{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "MobileInput",
    "$protected": false,
    "type": "object",
    "properties": {
        "mobile": {
            "type": "string",
            "pattern": "(^[+][1-9]{1,4}[0-9]{2,3}[0-9]{4,7}$)|^$"
        }
    },
    "required":[
        "user_name",
        "password"
    ]
}